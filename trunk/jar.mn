crashme.jar:
% locale  crashme en-US	%locale/en-US/
  locale/en-US/crashme.dtd                   (resources/locale/en-US/crashme.dtd)
% content crashme %content/crashme/
  content/crashme/firefoxOverlay.xul                   (resources/content/firefoxOverlay.xul)
  content/crashme/thunderbirdOverlay.xul                   (resources/content/thunderbirdOverlay.xul)
  content/crashme/seamonkeyOverlay.xul                   (resources/content/seamonkeyOverlay.xul)
  content/crashme/overlay.js                   (resources/content/overlay.js)
% overlay	chrome://browser/content/browser.xul	chrome://crashme/content/firefoxOverlay.xul
% overlay	chrome://messenger/content/messenger.xul	chrome://crashme/content/thunderbirdOverlay.xul
% overlay       chrome://navigator/content/navigatorOverlay.xul chrome://crashme/content/seamonkeyOverlay.xul